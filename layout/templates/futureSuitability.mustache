<div id="futureSuitability-keynote">
  <div class="keynote">
		<h2>{{ futureSuitability-keynote-1 }}</h2>
		<h2>{{ futureSuitability-keynote-2 }}</h2>
  </div>
</div>

<div id="futureSuitability" class="center">
	  <ul class='list'>
	  {{#futureSuitability-bullets}}  <li>{{.}}</li>
	  {{/futureSuitability-bullets}}</ul>
	  <div class="figToggle showing" id="suitability"><p>Lake Count</p></div> 
	  <div class="figToggle" id="figArea"><p>Total Lake Area</p></div>
	  <object id="futureSuitabilityFig" type="image/svg+xml" class="svgFig" data="futureSuitability-desktop.svg"></object>
	  <object id="futureSuitabilityFigArea" type="image/svg+xml" class="svgFig" data="futureSuitabilityFigArea-desktop.svg"></object>
	  <div class="captionContainer">
	    <p class="caption">{{ futureSuitability-caption }}</p>
	  </div>
	  <br/>
	  <h1>{{ futureSuitability-explore-h }}</h1>
	  <div class="exploreMore">
	  <a href="explore/map.html" target="_blank" class="ui-link">
	    <img src="images/fishMap-thumbnail.gif" alt="Detailed state map of predicted species" id="fishMapExplore">
	   </a>
	    <p>{{ futureSuitability-explore }}</p>
	    <a href="explore/map.html" target="_blank" class="ui-link">
	      <button id="explore-map" class="extraButton ui-btn ui-shadow ui-corner-all">Explore</button>
	    </a>
	  </div>
</div>
